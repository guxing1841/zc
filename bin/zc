import os, sys
ZC_BASEDIR = os.path.dirname(os.path.dirname(os.path.abspath(__file__)))
module_dir = ZC_BASEDIR + '/libs'
sys.path.insert(0, module_dir)
import zc_core

def main():
	zc_core.run()
	os._exit(0)
if __name__ == "__main__":
	main()
